datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id             Int     @id @default(autoincrement())
  email          String  @unique
  password       String
  accessLevel    String
  name           String
  status         String
  ra             String  @unique
  profilePicture String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
